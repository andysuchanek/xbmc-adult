<?xml version="1.0" encoding="utf-8"?><scraper framework="1.1" date="2010-02-22" name="XONAIR" content="movies" thumb="XONAIR.gif" language="en">
        <!--Thanks to DaGnome on XBMC Forums for this NfoUrl chunk!-->
	<NfoUrl dest="3">
		<RegExp input="$$1" output="&lt;url&gt;http://www.xonair.com/detail.cfm\1.htm&lt;/url&gt;" dest="3">
			<expression noclean="1">http://www.xonair.com/detail.cfm?([^.]*)</expression>
		</RegExp>
	</NfoUrl>

	<CreateSearchUrl dest="3">
		<RegExp input="$$1" output="&lt;url&gt;http://www.xonair.com/search.cfm?lang=US&amp;amp;pid=home&amp;amp;str=\1&amp;amp;view=all&amp;amp;searchby=title&amp;amp;x=0&amp;amp;y=0&lt;/url&gt;" dest="3">
			<expression noclean="1" />
		</RegExp>
	</CreateSearchUrl>
	<GetSearchResults dest="6">
		<!--RESULTS-->
		<RegExp input="$$5" output="&lt;?xml version=&quot;1.0&quot; encoding=&quot;iso-8859-1&quot; standalone=&quot;yes&quot;?&gt;&lt;results&gt;\1&lt;/results&gt;" dest="6">
			<!--SEARCH RESULT-->
			<RegExp input="$$1" output="&lt;entity&gt;&lt;title&gt;\2&lt;/title&gt;&lt;url&gt;http://www.xonair.com/detail.cfm?id=\1&amp;amp;pid=home&amp;amp;lang=US&lt;/url&gt;&lt;/entity&gt;" dest="5+">
				<expression repeat="yes" trim="1">&lt;a href="detail.cfm\?id=([\d]{1,10})&amp;pid=home&amp;lang=US" style=\"text-decoration:none; color:CC0000\"&gt;&lt;b&gt;(.+?)&lt;/b&gt;&lt;/a&gt;</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetSearchResults>
	<GetDetails dest="3">
		<!--DETAILS-->
		<RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="3">
			<!--STUDIO-->
			<RegExp input="$$1" output="&lt;studio&gt;\1&lt;/studio&gt;" dest="5+">
				<expression trim="1">Studio:&lt;/b&gt;&lt;/td&gt;\s+&lt;td&gt;&lt;a href="searchStudio\.cfm\?studio=\d+\&amp;pid=home\&amp;lang=US" style=\"text-decoration:none\"&gt;(.+?)&lt;/a&gt;&lt;/td&gt;</expression>
			</RegExp>
			<!--THUMB-FRONT-->
			<RegExp input="$$1" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="5+">
				<expression>&lt;img src=\"(http://image\d+\.imageservers\.com/pdimages/\d+/\d+\.jpg)\" border=\"0\"&gt;</expression>
			</RegExp>
			<!--THUMB-BACK-->
			<RegExp input="$$1" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="5+">
				<expression>&lt;img src=\"(http://image\d+\.imageservers\.com/pdimages/\d+/\d+_p\.jpg)\" border=\"0\"&gt;</expression>
			</RegExp>
			<!--TITLE-->
			<RegExp input="$$1" output="&lt;title&gt;\1&lt;/title&gt;" dest="5+">
				<expression trim="1">&lt;title&gt;Xonair\.com - (.+?)&lt;/title&gt;</expression>
			</RegExp>
			<!--GENRE-->
			<RegExp input="$$1" output="&lt;genre&gt;\1&lt;/genre&gt;" dest="5+">
				<expression repeat="yes" trim="1">&lt;a href=\"searchGenre\.cfm\?genre=\d+&amp;pid=home&amp;lang=US" style=\"text-decoration:none\"&gt;(.+?)&lt;/a&gt;</expression>
			</RegExp>
			<!--YEAR-->
			<RegExp input="$$1" output="&lt;year&gt;\1&lt;/year&gt;" dest="5+">
				<expression>Release Date:&lt;/b&gt;&lt;/td&gt;\s+&lt;td&gt;\d{2}/\d{2}/(\d{4})&lt;/td&gt;</expression>
			</RegExp>
			<!--PLOT-->
			<RegExp input="$$1" output="&lt;plot&gt;\1&lt;/plot&gt;" dest="5+">
				<expression>&lt;b&gt;Story&lt;/b&gt;.+?&lt;td&gt;(.*?)&lt;/td&gt;</expression>
			</RegExp>
			<!--DIRECTOR-->
			<RegExp input="$$1" output="&lt;director&gt;\1&lt;/director&gt;" dest="5+">
				<expression trim="1">Director:&lt;/b&gt;&lt;/td&gt;\s+&lt;td&gt;&lt;a href="/search\.cfm\?lang=US&amp;pid=home&amp;str=.+?&amp;view=all&amp;searchby=director"&gt;(.+?)&lt;/a&gt;&lt;/td&gt;</expression>
			</RegExp>
			<!--ACTORS-->
			<RegExp input="$$1" output="&lt;actor&gt;&lt;name&gt;\2&lt;/name&gt;&lt;thumb&gt;http://xonair.imageservers.com/xoa/performer/\1_m.jpg&lt;/thumb&gt;&lt;/actor&gt;" dest="5+">
				<expression repeat="yes" trim="1,2">searchPerf\.cfm\?perf=(\d+)&amp;pid=home&amp;lang=US\" style=\"text-decoration:none\"&gt;(.+?)&lt;/a&gt;</expression>
			</RegExp>
			<!--RUNTIME-->
			<RegExp input="$$1" output="&lt;runtime&gt;\1&lt;/runtime&gt;" dest="5+">
				<expression>Length:&lt;/b&gt;&lt;/td&gt;\s+&lt;td&gt;(.*?)&lt;/td&gt;</expression>
			</RegExp>
			<expression noclean="1" />
		</RegExp>
	</GetDetails>
</scraper>